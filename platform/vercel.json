{
  "routes": [
    {
      "src": "/",
      "dest": "/index.html",
      "headers": {
        "cache-control": "public, max-age=3600"
      }
    },
    {
      "src": "/api/health",
      "dest": "/api/health.js",
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    },
    {
      "src": "/api/health/all",
      "dest": "/api/health/all.js"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ]
}
